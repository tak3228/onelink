<%= form_for(@link) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
<div class="field">
    <%= f.text_field :url, placeholder: "URL..." %>
    <%= f.text_field :title, placeholder: "title..." %>
    <%= f.text_area :bunrui, placeholder: "Compose new micropost..." %>
</div>
<%= f.submit "作成", class: "btn btn-primary" %>
<% end %>
